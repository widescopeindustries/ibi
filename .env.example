# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Stripe Price IDs
STRIPE_PRO_MONTHLY_PRICE_ID=price_xxxxx

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Analytics Configuration
# Google Analytics 4 (GA4) - Get your Measurement ID from https://analytics.google.com
# Format: G-XXXXXXXXXX
NEXT_PUBLIC_GA_MEASUREMENT_ID=your_ga4_measurement_id

# Meta Pixel (Facebook Pixel) - Get your Pixel ID from https://business.facebook.com/events_manager
# Format: numeric ID (e.g., 1234567890123456)
NEXT_PUBLIC_META_PIXEL_ID=your_meta_pixel_id

# Email Configuration (Resend)
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxxxxxx
EMAIL_FROM_ADDRESS=IBI Directory <noreply@yourdomain.com>
